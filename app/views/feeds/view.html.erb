 <container> <inner>

<% if @editor %>
    <%= render partial: "header_editor", locals: { feed: @feed } %>
<% else %>
    <%= render partial: "header_viewer", locals: { feed: @feed } %>
<%end %>

</inner></container> 


<feed>
    <mesh class="feed-inner gap4">
        <% @posts.each do |post| %>
            <%= render partial: "posts/post", locals: { post: post } %>
        <% end %>
    </mesh>
</feed>

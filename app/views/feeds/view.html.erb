<container>

<% if @editor %>
    <%= render partial: "header_editor", locals: { feed: @feed } %>
<% else %>
    <%= render partial: "header_viewer", locals: { feed: @feed } %>
<%end %>

</container>

<feed>
    <% @posts.sort_by { rand }.each do |post| %>
        <%= render partial: "posts/post", locals: { post: post } %>
    <% end %>
</feed>
